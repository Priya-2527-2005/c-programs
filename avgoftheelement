#include <stdio.h>

int main() {
    int a[10],n,i,sum=0;
    float avg;
printf("enter size:");
scanf("%d",&n);
printf("enter the elements:\n");
for(i=0;i<n;++i)
{
    scanf("%d",&a[i]);
}
for(i=0;i<n;++i){
    sum=sum+a[i];
}
avg=sum/n;
printf("the avg of the elements of the array %f",avg);
return 0;
}
