#include <stdio.h>

int main() {
    int a[10],n,i,min;
printf("enter size:");
scanf("%d",&n);
printf("enter the elements:\n");
for(i=0;i<n;++i)
{
    scanf("%d",&a[i]);
}
min=a[0];
for(i=0;i<n;++i){
   if(a[i]<min)
   {
       min=a[i];
   }
}
printf("the minimum elements of the array is:%d",min);
return 0;
}
