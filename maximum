#include <stdio.h>

int main() {
    int a[10],n,i,max;
printf("enter size:");
scanf("%d",&n);
printf("enter the elements:\n");
for(i=0;i<n;++i)
{
    scanf("%d",&a[i]);
}
max=a[0];
for(i=0;i<n;++i){
   if(a[i]>max)
   {
       max=a[i];
   }
}
printf("the maximum elements of the array is:%d",max);
return 0;
}
