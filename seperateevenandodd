#include <stdio.h>

void main() {
    int a[10],b[10],c[10],j=0,k=0,n,i;
    printf("enter the size");
  scanf("%d",&n);
  printf("the elements of an array:\n");
  for(i=0;i<n;i++)
  {
  scanf("%d",&a[i]);
  }
 for(i=0;i<n;i++)
 {
     if(a[i]%2==0)
 {
 b[i]=a[i];
 j++;
}
else
{
    c[k]=b[i];
    k++;
}
for(i=0;i<j;i++)
{
    printf("%d",b[i]);
}
for(i=0;i<k;i++)
{
    printf("%d",c[i]);
}
}
}
